@using Event.Models.ViewModels
@model List<EventDisplayViewModel>
@{
    Layout = "_Layout";
}
<section class="events-page">
    @if (Model.Count == 0)
    {
        <h1>No Match found</h1>
    }
    else
    {
        <div>
            <h1>Found Matches</h1>
        </div>
        <div id="events">
        @foreach (var item in Model)
        {

            <partial name="EventCard" , model="item" />
        }
        </div>
    }
   
</section>
